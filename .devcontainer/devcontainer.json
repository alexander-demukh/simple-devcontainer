{
	"name": "Go Dev Container",
	"dockerFile": "Dockerfile",
	"forwardPorts": [8000],
	"runArgs": ["-p", "8000:8000"],
	"workspaceFolder": "/workspace",
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
	"postCreateCommand": "go mod download",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go"
			]
		}
	}
}
